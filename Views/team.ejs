<!DOCTYPE html>
<html lang="en">
  <head>
    <meta charset="UTF-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>Team Members - Almira Properties</title>
    <link rel="stylesheet" href="/css/homepage.css" />
    <link rel="shortcut icon" href="/icons/business-outline.svg" type="image/svg" />
  </head>
  <body>
   <!-- Include Navigation Partial -->
  <%- include('partials/nav') %>

  <!-- Include Header Partial -->
  <%- include('partials/header', { title: 'Team' }) %>
  

  <div class="mainTeam">
    <div class="teamContent">
      <% teamMembers.forEach(member => { %>
        <div class="teamMembers">
          <h2><%= member.name %></h2>
          <p>Realtor #: <%= member.realtorNumber %></p>
          <img src="<%= member.image %>" alt="<%= member.name %> Photo" class="teamMember_photo" />
          <p>Email: <%= member.email %></p>
          <p>Phone: <%= member.phone %></p>
        </div>
      <% }); %>
    </div>
  </div>

    <!-- Include Footer Partial -->
    <%- include('partials/footer') %>
  </body>
</html>
